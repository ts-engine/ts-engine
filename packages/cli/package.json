{
  "name": "@ts-engine/cli",
  "version": "1.5.0",
  "description": "Power TypeScript packages and reduce your config overhead.",
  "repository": {
    "url": "https://github.com/ts-engine/ts-engine"
  },
  "license": "MIT",
  "keywords": [
    "typescript",
    "cli",
    "packaging",
    "bundling",
    "rollup",
    "linting",
    "eslint",
    "testing",
    "jest"
  ],
  "bin": {
    "ts-engine": "./dist/cli.js"
  },
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.build.json && cp -R src/tool-files dist && rm -rf node_modules && yarn",
    "lint": "ts-engine lint",
    "test": "ts-engine test",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@ts-engine/babel-preset": "1.5.0",
    "@ts-engine/eslint-config": "1.5.0",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^25.1.4",
    "@types/minimist": "^1.2.0",
    "@types/node": "^13.11.0",
    "builtin-modules": "^3.1.0",
    "chalk": "^4.0.0",
    "core-js": "^3.6.5",
    "eslint": "^7.0.0",
    "fs-extra": "^9.0.0",
    "glob": "^7.1.6",
    "glob-promise": "^3.4.0",
    "jest": "^25.2.4",
    "minimist": "^1.2.5",
    "ora": "^4.0.4",
    "prettier": "^2.0.2",
    "progress-estimator": "^0.2.2",
    "regenerator-runtime": "^0.13.5",
    "rollup": "^2.3.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-preserve-shebangs": "^0.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "typescript": "^3.8.3",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@types/ora": "^3.2.0",
    "@types/validate-npm-package-name": "^3.0.0"
  }
}
